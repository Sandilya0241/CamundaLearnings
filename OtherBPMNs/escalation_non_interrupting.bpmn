      <bpmn:exclusiveGateway id="Gateway_0m6uacu" name="Shipping done within a day">
        <bpmn:incoming>Flow_00368xl</bpmn:incoming>
        <bpmn:outgoing>Flow_1lnqhp3</bpmn:outgoing>
        <bpmn:outgoing>Flow_0ryqrdo</bpmn:outgoing>
