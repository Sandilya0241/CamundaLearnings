        <bpmn:extensionElements>
          <camunda:formData>
            <camunda:formField id="shippingDays" label="shippingDays" type="long" />
          </camunda:formData>
        </bpmn:extensionElements>
        <bpmn:incoming>Flow_0ywg8zo</bpmn:incoming>
        <bpmn:outgoing>Flow_00368xl</bpmn:outgoing>
